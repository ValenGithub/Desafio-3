<h1>Carrito de Compras</h1>


<div class="cartContainer">
  <h4>Productos en el carrito:</h4>
  <ul class="cartList">
    {{#each renderCart.products}}
    <li class="cartItem">
      <h5>Producto: {{this.product.title}}</h5>
      <p>Cantidad: {{this.quantity}}</p>
    </li>
    {{/each}}
  </ul>
</div>

<script src="/js/index.js"></script>